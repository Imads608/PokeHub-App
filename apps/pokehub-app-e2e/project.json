{
  "name": "pokehub-app-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/pokehub-app-e2e/src",
  "implicitDependencies": ["pokehub-app"],
  "targets": {
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "outputs": ["{workspaceRoot}/dist/.playwright/apps/pokehub-app-e2e"],
      "options": {
        "config": "apps/pokehub-app-e2e/playwright.config.ts"
      }
    },
    "e2e:verbose": {
      "executor": "nx:run-commands",
      "options": {
        "command": "DEBUG=pw:webserver nx e2e pokehub-app-e2e {args}",
        "forwardAllArgs": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
