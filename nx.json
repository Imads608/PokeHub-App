{
  "npmScope": "pokehub",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api-gateway": {
      "tags": []
    },
    "auth": {
      "tags": []
    },
    "auth-service": {
      "tags": []
    },
    "chat-service": {
      "tags": []
    },
    "events": {
      "tags": []
    },
    "react-app": {
      "tags": []
    },
    "react-app-e2e": {
      "tags": [],
      "implicitDependencies": ["react-app"]
    },
    "room": {
      "tags": []
    },
    "socket-service": {
      "tags": []
    },
    "user": {
      "tags": []
    },
    "user-service": {
      "tags": []
    }
  }
}
