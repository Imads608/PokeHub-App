{
  "npmScope": "pokehub",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api-gateway": {
      "tags": []
    },
    "auth": {
      "tags": []
    },
    "auth-service": {
      "tags": []
    },
    "chat-service": {
      "tags": []
    },
    "events": {
      "tags": []
    },
    "next-app": {
      "tags": []
    },
    "next-app-e2e": {
      "tags": [],
      "implicitDependencies": ["next-app"]
    },
    "react-app": {
      "tags": []
    },
    "react-app-e2e": {
      "tags": [],
      "implicitDependencies": ["react-app"]
    },
    "room": {
      "tags": []
    },
    "socket-service": {
      "tags": []
    },
    "user": {
      "tags": []
    },
    "user-service": {
      "tags": []
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "api-gateway"
}
