      ],
      "@pokehub/frontend/pokehub-nav-components": [
        "packages/frontend/pokehub-nav-components/src/index.ts"
      ],
      "@pokehub/frontend/shared-app-bootstrapper": [
        "packages/frontend/shared-app-bootstrapper/src/index.ts"
      ],
      "@pokehub/frontend/shared-app-router": [
        "packages/frontend/shared-app-router/src/index.ts"
      ],
      "@pokehub/frontend/shared-auth-context": [
        "packages/frontend/shared-auth-context/src/index.ts"
      ],
      "@pokehub/frontend/shared-context": [
        "packages/frontend/shared-context/src/index.ts"
      ],
      "@pokehub/frontend/shared-data-provider": [
        "packages/frontend/shared-data-provider/src/index.ts"
