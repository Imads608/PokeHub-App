      ],
      "@pokehub/frontend/shared-ui-components": [
        "packages/frontend/shared-ui-components/src/index.ts"
      ],
      "@pokehub/frontend/shared-ui-icons": [
        "packages/frontend/shared-ui-icons/src/index.ts"
      ],
      "@pokehub/shared/shared-auth-models": [
        "packages/shared/shared-auth-models/src/index.ts"
      ],
      "@pokehub/shared/shared-user-models": [
        "packages/shared/shared-user-models/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
